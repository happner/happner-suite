          "type": "string"
        },
        "defaultAuthProvider": {
          "type": "string"
        },
        "defaultNonceTTL": {
          "type": "number"
        },
        "disableDefaultAdminNetworkConnections": {
          "type": "boolean"
        },
        "expiry_grace": {
          "type": "number"
