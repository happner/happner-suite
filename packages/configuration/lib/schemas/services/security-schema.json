{
  "$id": "security-schema.json",
  "$ref": "#/definitions/ServicesSecurity",
  "definitions": {
    "ServicesSecurity": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "config": {
          "$ref": "#/definitions/SecurityConfig"
        },
        "disableDefaultAdminNetworkConnections": {
          "type": "boolean"
        },
        "path": {
          "type": "string"
        }
      },
      "required": [
      ],
      "title": "ServicesSecurity"
    },
    "SecurityConfig": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^(__)?cache_checkpoint_authorization$": {
          "$ref": "#/definitions/Cache"
        },
        "^(__)?cache_groups$": {
          "$ref": "#/definitions/Cache"
        },
        "^(__)?cache_groups_by_user$": {
          "$ref": "#/definitions/Cache"
        },
        "^(__)?cache_permissions$": {
          "$ref": "#/definitions/Cache"
        },
        "^(__)?cache_users$": {
          "$ref": "#/definitions/Cache"
        },
        "^(__)?cache_users_by_groups$": {
          "$ref": "#/definitions/Cache"
        }
      },
      "properties": {
        "accountLockout": {
          "$ref": "#/definitions/AccountLockout"
        },
        "activateSessionManagement": {
          "type": "boolean"
        },
        "adminGroup": {
          "$ref": "#/definitions/AdminGroup"
        },
        "adminUser": {
          "$ref": "#/definitions/AdminUser"
        },
        "allowAnonymousAccess": {
          "type": "boolean"
        },
        "authProviders": {
          "$ref": "#/definitions/AuthProviders"
        },
        "cookieName": {
          "type": "string"
        },
        "cookieDomain": {
          "type": "string"
        },
        "defaultAuthProvider": {
          "type": "string"
        },
        "defaultNonceTTL": {
          "type": "number"
        },
        "disableDefaultAdminNetworkConnections": {
          "type": "boolean"
        },
        "expiry_grace": {
          "type": "number"
        },
        "groupPermissionsPolicy": {
          "type": "string"
        },
        "httpsCookie": {
          "type": "boolean"
        },
        "keyPair": {
          "$ref": "#/definitions/KeyPair"
        },
        "lockTokenToLoginType": {
          "type": "boolean"
        },
        "lockTokenToUserId": {
          "type": "boolean"
        },
        "logSessionActivity": {
          "type": "boolean"
        },
        "persistPermissions": {
          "type": "boolean"
        },
        "pbkdf2Iterations": {
          "type": "integer"
        },
        "profiles": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/Profile"
          }
        },
        "secure": {
          "type": "boolean"
        },
        "sessionActivityTTL": {
          "type": "number"
        },
        "sessionTokenSecret": {
          "type": "string"
        },
        "updateSubscriptionsOnSecurityDirectoryChanged": {
          "type": "boolean"
        },
        "usernamesCaseInsensitive": {
          "type": "boolean"
        }
      },
      "required": [
      ],
      "title": "SecurityConfig"
    },
    "AccountLockout": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "attempts": {
          "type": "integer",
          "minimum": 1,
          "maximum": 20
        },
        "enabled": {
          "type": "boolean"
        },
        "retryInterval": {
          "type": "integer",
          "minimum": 1000,
          "maximum": 1000000
        }
      },
      "required": [
      ],
      "title": "AccountLockout"
    },
    "AdminGroup": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "custom_data": {
          "$ref": "#/definitions/CustomData"
        },
        "name": {
          "type": "string"
        },
        "permissions": {
          "$ref": "#/definitions/Permission"
        }
      },
      "required": [
      ],
      "title": "AdminGroup"
    },
    "AdminUser": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "username": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "custom_data": {
          "$ref": "#/definitions/CustomData"
        },
        "publicKey": {
          "type": "string"
        }
      },
      "required": [
      ],
      "title": "AdminUser"
    },
    "AuthProviders": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "testProvider": {
          "$ref": "#/definitions/TestProvider"
        }
      },
      "required": [
      ],
      "title": "AuthProviders"
    },
    "Cache": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "max": {
          "type": "number"
        },
        "maxAge": {
          "type": "number"
        }
      },
      "required": [
      ],
      "title": "Cache"
    },
    "CustomData": {
      "type": "object",
      "properties": {
        "description": {
          "type": "string"
        }
      },
      "additionalProperties": false,
      "title": "CustomData"
    },
    "KeyPair": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "privateKey": {
          "type": "string"
        },
        "publicKey": {
          "type": "string"
        }
      },
      "title": "KeyPair"
    },
    "Permission": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        "^.*$": {
          "type": [
            "string",
            "object",
            "integer",
            "boolean"
          ]
        }
      },
      "required": [
      ],
      "title": "Permission"
    },
    "Profile": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "name": {
          "type": "string"
        },
        "policy": {
          "$ref": "#/definitions/Policy"
        },
        "session": {
          "$ref": "#/definitions/Session"
        }
      },
      "required": [
      ],
      "title": "Profile"
    },
    "Policy": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "disallowTokenLogins": {
          "type": "boolean"
        },
        "lockTokenToOrigin": {
          "type": "boolean"
        },
        "sourceIPWhitelist": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "ttl": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "string",
              "pattern": "\\d+ (milliseconds|seconds|minutes|hours|days|weeks|months|years)"
            }
          ]
        },
        "inactivity_threshold": {
          "anyOf": [
            {
              "type": "integer"
            },
            {
              "type": "string",
              "pattern": "\\d+ (milliseconds|seconds|minutes|hours|days|weeks|months|years)"
            }
          ]
        },
        "usage_limit": {
          "type": [
            "number"
          ]
        }
      },
      "required": [
      ],
      "title": "Policy"
    },
    "Session": {
      "type": "object",
      "additionalProperties": true,
      "title": "Session"
    },
    "TestProvider": {
      "type": "object",
      "additionalProperties": false,
      "title": "TestProvider"
    }
  }
}
