{
  "name": "happner-cluster",
  "version": "13.0.0-prerelease-5",
  "description": "Extends happner with clustering capabilities",
  "main": "index.js",
  "scripts": {
    "test-integration-1": "mocha silence.js test/integration/0*  test/integration/1* --exit",
    "test-integration-2": "mocha silence.js test/integration/2* test/integration/3* test/integration/4*  --exit",
    "test-unit": "mocha silence.js test/unit/* --recursive --exit",
    "test": "mocha silence.js test/unit/* test/integration/* --recursive --exit",
    "test-cover": "nyc --reporter=lcov --reporter=text --reporter=html npm test",
    "test-cover-unit": "nyc --reporter=lcov --reporter=text --reporter=html npm run test-unit",
    "test-cover-integration-1": "nyc --reporter=lcov --reporter=text --reporter=html npm run test-integration-1",
    "test-cover-integration-2": "nyc --reporter=lcov --reporter=text --reporter=html npm run test-integration-2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/happner/happner-cluster.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/happner/happner-cluster/issues"
  },
  "homepage": "https://github.com/happner/happner-cluster#readme",
  "devDependencies": {
    "array-unique": "^0.3.2",
    "happn-commons-test": "^1.0.4",
    "intercept-stdout": "^0.1.2",
    "restler": "^3.4.0"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "happn-cluster": "^13.0.0-prerelease-5",
    "happn-commons": "^1.1.0",
    "happn-db-provider-mongo": "^1.0.11-prerelease-1",
    "happner-2": "^12.1.8-prerelease-1",
    "happner-client": "^12.2.7-prerelease-1",
    "hashring": "^3.2.0",
    "http-proxy": "1.18.1",
    "semver": "^5.3.0"
  }
}
